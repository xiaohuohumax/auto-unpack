{
    "$defs": {
        "ArchivePluginConfig": {
            "properties": {
                "name": {
                    "const": "archive",
                    "default": "archive",
                    "description": "压缩包处理插件",
                    "title": "Name",
                    "type": "string"
                },
                "load_key": {
                    "default": "default",
                    "description": "上下文加载 key",
                    "title": "Load Key",
                    "type": "string"
                },
                "save_key": {
                    "default": "default",
                    "description": "上下文保存 key",
                    "title": "Save Key",
                    "type": "string"
                },
                "mode": {
                    "description": "压缩包处理模式\nlist: 列出压缩包内文件信息\nextract: 解压压缩包\ntest: 测试压缩包完整性",
                    "enum": [
                        "list",
                        "extract",
                        "test"
                    ],
                    "title": "Mode",
                    "type": "string"
                },
                "password_path": {
                    "description": "密码文件路径(默认: passwords.txt)",
                    "format": "path",
                    "title": "Password Path",
                    "type": "string"
                },
                "fail_key": {
                    "default": null,
                    "description": "失败上下文 key(默认: null)",
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "title": "Fail Key"
                },
                "stat_file_name": {
                    "default": null,
                    "description": "统计信息问件名(默认: null)",
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "title": "Stat File Name"
                },
                "thread_max": {
                    "default": 10,
                    "description": "线程池最大线程数(默认: 10)",
                    "title": "Thread Max",
                    "type": "integer"
                },
                "result_processing_mode": {
                    "default": "strict",
                    "description": "结果处理模式(默认: strict)\nstrict: 严格模式[结果绝对依靠 7-zip 命令行输出]\ngreedy: 贪婪模式[7-zip 返回某些错误码时, 也会尝试识别/测试/解压]",
                    "enum": [
                        "strict",
                        "greedy"
                    ],
                    "title": "Result Processing Mode",
                    "type": "string"
                },
                "output_dir": {
                    "description": "输出目录(默认: output)",
                    "format": "path",
                    "title": "Output Dir",
                    "type": "string"
                },
                "keep_dir": {
                    "default": true,
                    "description": "是否保留压缩包目录(默认: true)",
                    "title": "Keep Dir",
                    "type": "boolean"
                }
            },
            "required": [
                "mode"
            ],
            "title": "ArchivePluginConfig",
            "type": "object"
        },
        "CustomStep": {
            "properties": {
                "name": {
                    "description": "自定义插件",
                    "title": "Name",
                    "type": "string",
                    "not": {
                        "enum": [
                            "archive",
                            "empty",
                            "flat",
                            "log",
                            "remove",
                            "rename",
                            "scan",
                            "_template",
                            "transfer",
                            "filter",
                            "loop",
                            "merge",
                            "switch",
                            "print"
                        ]
                    }
                }
            },
            "required": [
                "name"
            ],
            "title": "CustomStep",
            "type": "object"
        },
        "EmptyPluginConfig": {
            "properties": {
                "name": {
                    "const": "empty",
                    "default": "empty",
                    "description": "空文件夹清理插件",
                    "title": "Name",
                    "type": "string"
                },
                "dir": {
                    "description": "要清理的目录",
                    "format": "path",
                    "title": "Dir",
                    "type": "string"
                }
            },
            "required": [
                "dir"
            ],
            "title": "EmptyPluginConfig",
            "type": "object"
        },
        "FilterPluginConfig": {
            "properties": {
                "name": {
                    "const": "filter",
                    "default": "filter",
                    "description": "过滤插件",
                    "title": "Name",
                    "type": "string"
                },
                "load_key": {
                    "default": "default",
                    "description": "上下文加载 key",
                    "title": "Load Key",
                    "type": "string"
                },
                "save_key": {
                    "default": "default",
                    "description": "上下文保存 key",
                    "title": "Save Key",
                    "type": "string"
                },
                "includes": {
                    "default": [
                        "**/*"
                    ],
                    "description": "包含的文件(glob 表达式, 默认: [**/*])",
                    "items": {
                        "type": "string"
                    },
                    "title": "Includes",
                    "type": "array"
                },
                "excludes": {
                    "default": [],
                    "description": "排除的文件(glob 表达式, 默认: [])",
                    "items": {
                        "type": "string"
                    },
                    "title": "Excludes",
                    "type": "array"
                },
                "exclude_key": {
                    "default": null,
                    "description": "排除掉的上下文(默认: null)",
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "title": "Exclude Key"
                },
                "rules": {
                    "default": [],
                    "description": "筛选规则(默认: [])",
                    "items": {
                        "oneOf": [
                            {
                                "$ref": "#/$defs/SizeFilter"
                            },
                            {
                                "$ref": "#/$defs/GlobFilter"
                            }
                        ]
                    },
                    "title": "Rules",
                    "type": "array"
                }
            },
            "title": "FilterPluginConfig",
            "type": "object"
        },
        "FlatPluginConfig": {
            "properties": {
                "name": {
                    "const": "flat",
                    "default": "flat",
                    "description": "扁平化文件夹插件",
                    "title": "Name",
                    "type": "string"
                },
                "dir": {
                    "description": "需要扁平化的文件夹",
                    "format": "path",
                    "title": "Dir",
                    "type": "string"
                },
                "depth": {
                    "default": null,
                    "description": "扁平化的深度(null: 不限制深度, 默认: null)",
                    "oneOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "title": "Depth"
                }
            },
            "required": [
                "dir"
            ],
            "title": "FlatPluginConfig",
            "type": "object"
        },
        "FlowConfig": {
            "description": "流程配置",
            "properties": {
                "steps": {
                    "description": "流程步骤",
                    "items": {
                        "oneOf": [
                            {
                                "$ref": "#/$defs/CustomStep"
                            },
                            {
                                "$ref": "#/$defs/ArchivePluginConfig"
                            },
                            {
                                "$ref": "#/$defs/EmptyPluginConfig"
                            },
                            {
                                "$ref": "#/$defs/FlatPluginConfig"
                            },
                            {
                                "$ref": "#/$defs/LogPluginConfig"
                            },
                            {
                                "$ref": "#/$defs/RemovePluginConfig"
                            },
                            {
                                "$ref": "#/$defs/RenamePluginConfig"
                            },
                            {
                                "$ref": "#/$defs/ScanPluginConfig"
                            },
                            {
                                "$ref": "#/$defs/TemplatePluginConfig"
                            },
                            {
                                "$ref": "#/$defs/TransferPluginConfig"
                            },
                            {
                                "$ref": "#/$defs/FilterPluginConfig"
                            },
                            {
                                "$ref": "#/$defs/LoopPluginConfig"
                            },
                            {
                                "$ref": "#/$defs/MergePluginConfig"
                            },
                            {
                                "$ref": "#/$defs/SwitchPluginConfig"
                            },
                            {
                                "$ref": "#/$defs/PrintPluginConfig"
                            }
                        ]
                    },
                    "title": "Steps",
                    "type": "array"
                }
            },
            "required": [
                "steps"
            ],
            "title": "FlowConfig",
            "type": "object"
        },
        "GlobCase": {
            "description": "文件 glob 表达式分支条件",
            "properties": {
                "save_key": {
                    "title": "Save Key",
                    "type": "string"
                },
                "mode": {
                    "const": "glob",
                    "default": "glob",
                    "description": "文件名过滤(glob 表达式)",
                    "title": "Mode",
                    "type": "string"
                },
                "includes": {
                    "default": [
                        "**/*"
                    ],
                    "description": "包含的文件(glob 表达式, 默认: [**/*])",
                    "items": {
                        "type": "string"
                    },
                    "title": "Includes",
                    "type": "array"
                },
                "excludes": {
                    "default": [],
                    "description": "排除的文件(glob 表达式, 默认: [])",
                    "items": {
                        "type": "string"
                    },
                    "title": "Excludes",
                    "type": "array"
                }
            },
            "required": [
                "save_key"
            ],
            "title": "GlobCase",
            "type": "object"
        },
        "GlobFilter": {
            "description": "文件名过滤",
            "properties": {
                "mode": {
                    "const": "glob",
                    "default": "glob",
                    "description": "文件名过滤(glob 表达式)",
                    "title": "Mode",
                    "type": "string"
                },
                "includes": {
                    "default": [
                        "**/*"
                    ],
                    "description": "包含的文件(glob 表达式, 默认: [**/*])",
