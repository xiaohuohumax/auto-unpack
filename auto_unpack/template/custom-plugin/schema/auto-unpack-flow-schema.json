        "TransferPluginConfig": {
            "properties": {
                "name": {
                    "const": "transfer",
                    "default": "transfer",
                    "description": "转移插件",
                    "title": "Name",
                    "type": "string"
                },
                "load_key": {
                    "default": "default",
                    "description": "上下文加载 key",
                    "title": "Load Key",
                    "type": "string"
                },
                "save_key": {
                    "default": "default",
