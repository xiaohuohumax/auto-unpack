            "title": "GlobFilter",
            "type": "object"
        },
        "LogPluginConfig": {
            "properties": {
                "name": {
                    "const": "log",
                    "default": "log",
                    "description": "日志插件",
                    "title": "Name",
                    "type": "string"
                },
                "load_key": {
                    "default": "default",
                    "description": "上下文加载 key",
                    "title": "Load Key",
                    "type": "string"
                },
                "file_name": {
                    "default": "log",
                    "description": "日志文件名(默认: log)",
                    "title": "File Name",
                    "type": "string"
                }
            },
            "title": "LogPluginConfig",
            "type": "object"
        },
        "LoopPluginConfig": {
            "properties": {
                "name": {
                    "const": "loop",
                    "default": "loop",
                    "description": "循环插件",
                    "title": "Name",
                    "type": "string"
                },
                "load_key": {
                    "default": "default",
                    "description": "上下文加载 key",
                    "title": "Load Key",
                    "type": "string"
                },
                "steps": {
                    "description": "循环步骤",
                    "items": {
                        "oneOf": [
                            {
                                "$ref": "#/$defs/CustomStep"
                            },
                            {
                                "$ref": "#/$defs/ArchivePluginConfig"
                            },
                            {
                                "$ref": "#/$defs/EmptyPluginConfig"
                            },
                            {
                                "$ref": "#/$defs/FlatPluginConfig"
                            },
                            {
                                "$ref": "#/$defs/LogPluginConfig"
                            },
                            {
                                "$ref": "#/$defs/RemovePluginConfig"
                            },
                            {
                                "$ref": "#/$defs/RenamePluginConfig"
                            },
                            {
                                "$ref": "#/$defs/ScanPluginConfig"
                            },
                            {
                                "$ref": "#/$defs/TemplatePluginConfig"
                            },
                            {
                                "$ref": "#/$defs/TransferPluginConfig"
                            },
                            {
                                "$ref": "#/$defs/FilterPluginConfig"
                            },
                            {
                                "$ref": "#/$defs/LoopPluginConfig"
                            },
                            {
                                "$ref": "#/$defs/MergePluginConfig"
                            },
                            {
                                "$ref": "#/$defs/SwitchPluginConfig"
                            },
                            {
                                "$ref": "#/$defs/PrintPluginConfig"
                            }
                        ]
                    },
                    "title": "Steps",
                    "type": "array"
                },
                "max_loops": {
                    "default": 1024,
                    "description": "最大循环次数(-1: 不限制, 默认: 1024)",
                    "title": "Max Loops",
                    "type": "integer"
                },
                "loop_interval": {
                    "default": 1,
                    "description": "循环间隔时间(单位: 秒, 默认: 1)",
                    "title": "Loop Interval",
                    "type": "integer"
                }
            },
            "required": [
                "steps"
            ],
            "title": "LoopPluginConfig",
            "type": "object"
        },
        "MergePluginConfig": {
            "properties": {
                "name": {
                    "const": "merge",
                    "default": "merge",
                    "description": "合并上下文插件",
                    "title": "Name",
                    "type": "string"
                },
                "save_key": {
                    "default": "default",
                    "description": "上下文保存 key",
                    "title": "Save Key",
                    "type": "string"
                },
                "context_keys": {
                    "default": [],
                    "description": "需要合并的上下文 key 集合(默认: [])",
                    "items": {
                        "type": "string"
                    },
                    "title": "Context Keys",
                    "type": "array"
                }
            },
            "title": "MergePluginConfig",
            "type": "object"
        },
        "PrintPluginConfig": {
            "properties": {
                "name": {
                    "const": "print",
                    "default": "print",
                    "description": "打印插件",
                    "title": "Name",
                    "type": "string"
                },
                "message": {
                    "default": "Hello, world!",
                    "description": "打印的消息",
                    "title": "Message",
                    "type": "string"
                }
            },
            "title": "PrintPluginConfig",
            "type": "object"
        },
        "ReRule": {
            "description": "改名规则：正则表达式",
            "properties": {
                "mode": {
                    "const": "re",
                    "default": "re",
                    "description": "正则表达式模式",
                    "title": "Mode",
                    "type": "string"
                },
                "pattern": {
                    "description": "正则表达式",
                    "title": "Pattern",
                    "type": "string"
                },
                "replace": {
                    "description": "替换字符串",
                    "title": "Replace",
                    "type": "string"
                },
                "count": {
                    "default": 0,
                    "description": "替换次数(0: 不限次数, 默认: 0)",
                    "title": "Count",
                    "type": "integer"
                },
                "flags": {
                    "default": "",
                    "description": "正则表达式匹配模式(默认: '')",
                    "title": "Flags",
                    "type": "string"
                }
            },
            "required": [
                "pattern",
                "replace"
            ],
            "title": "ReRule",
            "type": "object"
        },
        "RemovePluginConfig": {
            "properties": {
                "name": {
                    "const": "remove",
                    "default": "remove",
                    "description": "删除文件插件",
                    "title": "Name",
                    "type": "string"
                },
                "load_key": {
                    "default": "default",
                    "description": "上下文加载 key",
                    "title": "Load Key",
                    "type": "string"
                }
            },
            "title": "RemovePluginConfig",
            "type": "object"
