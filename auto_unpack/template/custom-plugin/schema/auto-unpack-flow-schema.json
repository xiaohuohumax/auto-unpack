                },
                "load_key": {
                    "default": "default",
                    "description": "上下文加载 key",
                    "title": "Load Key",
                    "type": "string"
                },
                "save_key": {
                    "default": "default",
                    "description": "上下文保存 key",
                    "title": "Save Key",
                    "type": "string"
                },
                "rules": {
                    "default": [],
                    "description": "规则链(默认: [])",
                    "items": {
                        "oneOf": [
                            {
                                "$ref": "#/$defs/ReplaceRule"
                            },
                            {
                                "$ref": "#/$defs/ReRule"
                            }
                        ]
                    },
                    "title": "Rules",
                    "type": "array"
                }
            },
            "title": "RenamePluginConfig",
            "type": "object"
        },
        "ReplaceRule": {
            "description": "改名规则：替换",
            "properties": {
                "mode": {
                    "const": "replace",
                    "default": "replace",
                    "description": "替换模式",
                    "title": "Mode",
                    "type": "string"
                },
                "search": {
                    "description": "匹配字符串",
                    "title": "Search",
                    "type": "string"
                },
                "replace": {
                    "description": "替换字符串",
                    "title": "Replace",
                    "type": "string"
                },
                "count": {
                    "default": -1,
                    "description": "替换次数(-1: 全部替换, 默认: -1)",
